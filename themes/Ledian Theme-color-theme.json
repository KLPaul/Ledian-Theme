	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"string.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#8072A1"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder",
				"identifier"
			],
			"settings": {
				"foreground": "#FFD580"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color",
				"constant"
			],
			"settings": {
				"foreground": "#A3BEFF"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF6B6B"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#C694FF",
				"fontStyle": "bold"
			}
		},
